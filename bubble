#include<stdio.h>
void bubblesort(int a[],int n)
{int i,j,s;
	for (i=0;i<n-1;i++)
 	{
		for(j=0;j<n-i-1;j++)
		  	if(a[j]>a[j+1])
		  	  {
		  	  	s = a[j];
		  	  	a[j]=a[j+1];
		  	  	a[j+1]=s;
		    	}
	}}
int main()
{
	int i,j,n,s;
	printf("enter size\n");
	scanf("%d",&n);
	int a[n];
		 printf("enter number\n");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
   bubblesort(a,n);
   	printf("After selection sort-\n");
	for(i=0;i<n;i++)
	printf("%d\t",a[i]);
return 0;
}
